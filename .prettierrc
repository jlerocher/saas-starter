{
    "printWidth": 80,
    "bracketSpacing": true,
    "tabWidth": 4,
    "proseWrap": "always",
    "useTabs": false,
    "singleQuote": false,
    "semi": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-organize-imports"
    ],
    "tailwindConfig": "tailwind.config.ts"
}
